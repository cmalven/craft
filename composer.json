{
  "require": {
    "craftcms/aws-s3": "2.0.3",
    "craftcms/ckeditor": "3.8.0",
    "craftcms/cms": "4.8.0",
    "marionnewlevant/twig-perversion": "^3.0",
    "mmikkel/cp-field-inspect": "^1.4",
    "nystudio107/craft-autocomplete": "^1.10",
    "nystudio107/craft-emptycoalesce": "^4.0",
    "nystudio107/craft-seomatic": "4.0.42",
    "putyourlightson/craft-blitz": "4.12.2",
    "putyourlightson/craft-sherlock": "4.4.1",
    "sebastianlenz/linkfield": "2.1.5",
    "spacecatninja/imager-x": "4.3.1",
    "spatie/craft-ray": "^2.0",
    "topshelfcraft/environment-label": "4.0.3",
    "topshelfcraft/wordsmith": "4.4.0",
    "vaersaagod/matrixmate": "2.1.4",
    "verbb/expanded-singles": "2.0.5",
    "vlucas/phpdotenv": "^5.4.0"
  },
  "require-dev": {
    "yiisoft/yii2-shell": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "allow-plugins": {
      "craftcms/plugin-installer": true,
      "yiisoft/yii2-composer": true
    },
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "8.0.2"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example.dev', '.env');\""
    ]
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.craftcms.com",
      "canonical": false
    }
  ]
}
