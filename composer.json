{
  "require": {
    "craftcms/aws-s3": "2.0.3",
    "craftcms/cms": "4.4.6.1",
    "craftcms/redactor": "3.0.4",
    "marionnewlevant/twig-perversion": "^3.0",
    "mmikkel/cp-field-inspect": "^1.4",
    "nystudio107/craft-autocomplete": "^1.10",
    "nystudio107/craft-emptycoalesce": "^4.0",
    "nystudio107/craft-seomatic": "4.0.23",
    "putyourlightson/craft-blitz": "4.4.4",
    "putyourlightson/craft-sherlock": "4.3.0",
    "sebastianlenz/linkfield": "2.1.5",
    "spacecatninja/imager-x": "4.1.12",
    "spatie/craft-ray": "^2.0",
    "topshelfcraft/environment-label": "^4.0",
    "topshelfcraft/wordsmith": "4.2.0",
    "vaersaagod/matrixmate": "^2.1",
    "verbb/expanded-singles": "2.0.1",
    "vlucas/phpdotenv": "^5.4.0"
  },
  "require-dev": {
    "yiisoft/yii2-shell": "^2.0.3"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "allow-plugins": {
      "craftcms/plugin-installer": true,
      "yiisoft/yii2-composer": true
    },
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "8.0.2"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example.dev', '.env');\""
    ]
  }
}
